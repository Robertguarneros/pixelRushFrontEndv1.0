<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/buttonStore"
        android:layout_width="319dp"
        android:layout_height="55dp"
        android:layout_margin="10dp"
        android:backgroundTint="@color/Nami_Tangerine"
        android:onClick="getAllObjectsFromStore"
        android:text="Store"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonUserInformation"
        android:layout_width="319dp"
        android:layout_height="55dp"
        android:layout_margin="10dp"
        android:backgroundTint="@color/Nami_Tangerine"
        android:onClick="UserProfile"
        android:text="User Info"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonStore"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>